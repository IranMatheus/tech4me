function calcularValorCompra(codigoAcao, valorAcao, quantidade) {
  // Calculando o valor total da compra
  const valorTotal = valorAcao * quantidade;

  // Retornando o valor total
  return valorTotal;
}

// Chamadas da função com diferentes valores
const compra1 = calcularValorCompra("AAPL", 170.50, 10);
const compra2 = calcularValorCompra("GOOG", 2600.00, 2);
const compra3 = calcularValorCompra("MSFT", 280.75, 5);

// Exibindo os resultados (opcional)
console.log(`Valor da compra 1: ${compra1}`);
console.log(`Valor da compra 2: ${compra2}`);
console.log(`Valor da compra 3: ${compra3}`);
